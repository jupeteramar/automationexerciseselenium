<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AllureSuite">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg"/>
    </listeners>

    <test name="AllTests">
        <packages>
            <package name="org.seleniumtest.tests"/>
        </packages>
    </test>

    <test name="Products Tests">
        <classes>
            <class name="org.seleniumtests.tests.ProductsTest"/>
        </classes>
    </test>
</suite>
